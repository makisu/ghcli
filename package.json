{
  "name": "ghcli",
  "version": "0.0.0",
  "author": "Makisu",
  "bin": {
    "ghclii": "./bin/run"
  },
  "bugs": "https://github.com/makisu/ghcli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@octokit/rest": "^18.0.0",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/node-fetch": "^2.5.7",
    "chalk": "^4.1.0",
    "cli-ux": "^5.4.9",
    "conf": "^7.0.1",
    "dotenv": "^8.2.0",
    "lodash.clonedeep": "^4.5.0",
    "node-fetch": "^2.6.0",
    "open": "^7.0.4",
    "ora": "^4.0.4",
    "simple-git": "^2.12.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^14.0.23",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/makisu/ghcli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ghcli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "makisu/ghcli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
